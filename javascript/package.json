{
  "name": "graylog-web-interface",
  "version": "1.3.0-SNAPSHOT",
  "description": "Graylog Web Interface",
  "author": "torch",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/graylog2/graylog2-web-interface.git"
  },
  "readme": "../README.md",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "test": "./node_modules/karma/bin/karma start karma.ci.conf.js",
    "prepare-dev": "./tools/prepare-dev",
    "watch": "cp templates/scripts.scala.html.dev.template ../app/views/build/scripts.scala.html && ./node_modules/.bin/webpack --watch --devtool eval --config webpack.dev.config.js",
    "hot": "cp templates/scripts.scala.html.hmr.template ../app/views/build/scripts.scala.html && ./node_modules/.bin/webpack-dev-server --hot --inline --config webpack.hmr.config.js --content-base ../public --devtool eval"
  },
  "dependencies": {
    "c3": "^0.4.11-rc4",
    "crossfilter": "1.3.x",
    "d3": "<=3.5.0",
    "dc": "2.0.0-beta.19",
    "immutable": "^3.7.5",
    "markdown": "^0.5.0",
    "moment": "2.10.6",
    "moment-duration-format": "1.3.0",
    "numeral": "1.5.x",
    "qs": "5.1.x",
    "react": "0.13.x",
    "react-bootstrap": "0.25.x",
    "react-zeroclipboard": "^2.0.1",
    "sockjs-client": "1.0.x"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "eslint": "^1.5.1",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.7.9",
    "eslint-plugin-react": "^3.4.2",
    "html-webpack-plugin": "^1.6.1",
    "jasmine-core": "*",
    "jquery": "2.1.x",
    "json-loader": "^0.5.3",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "*",
    "karma-html2js-preprocessor": "*",
    "karma-jasmine": "*",
    "karma-phantomjs-launcher": "*",
    "karma-webpack": "*",
    "phantomjs": ">=1.9",
    "react-hot-loader": "^1.3.0",
    "ts-loader": "^0.5.5",
    "typescript": "^1.6.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
