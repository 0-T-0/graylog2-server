{
  "name": "graylog2-web-interface",
  "version": "0.93.0",
  "description": "Graylog2 Web Interface",
  "author": "torch",
  "license": "GPL",
  "scripts": {
    "build": "./node_modules/.bin/gulp deploy-prod",
    "jest-test": "./node_modules/.bin/jest",
    "test": "./node_modules/.bin/gulp bundle-tests && ./node_modules/karma/bin/karma start karma.ci.conf.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/joi/"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "dc": "dc-js/dc.js",
    "crossfilter": "1.x",
    "d3": "3.x",
    "joi": "^4.6.2",
    "react": "^0.12.1",
    "markdown": "^0.5.0",
    "qs": "2.2.2"
  },
  "devDependencies": {
    "browserify": "*",
    "gulp": "*",
    "gulp-compile-handlebars": "*",
    "gulp-concat": "*",
    "gulp-debug": "*",
    "gulp-jshint": "*",
    "gulp-livereload": "*",
    "gulp-rename": "*",
    "gulp-rev": "*",
    "gulp-rimraf": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "jquery": "*",
    "jshint-stylish": "*",
    "rimraf": "*",
    "run-sequence": "*",
    "vinyl-source-stream": "*",
    "watchify": "*",
    "tsify": "*",

    "karma": "*",
    "karma-jasmine": "*",
    "karma-chrome-launcher": "*",
    "karma-html2js-preprocessor": "*",
    "karma-phantomjs-launcher": "*",

    "gulp-react": "^2.0.0",
    "gulp-jest": "^0.2.2",
    "jest-cli": "^0.1.18",
    "react-tools": "^0.12.1",
    "reactify": "0.16.*"
  }
}
