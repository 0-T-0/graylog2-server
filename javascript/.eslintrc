{
  "extends": [
    "eslint:recommended",
    "airbnb",
    "import/warnings"
  ],
  "plugins": [
    "import",
  ],
  "rules": {
    "no-else-return": 1,
    "new-cap": 0,
    "indent": [2, 2, { "SwitchCase" : 1}],
    "import/no-unresolved": 1,
  },
  "settings": {
    "import/resolve": {
      "extensions": [".js", ".jsx", ".ts"],
      "moduleDirectory": ["src", "node_modules"]
    }
  }
}

